{
    "name": "frontend",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite --host 0.0.0.0 --port 5175",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "npm test -- --coverage",
        "lint": "eslint ./src --ext .ts,.tsx,.js,.jsx,.cjs",
        "format": "prettier --write ./src",
        "copy:icons": "cp -rf ./node_modules/@shoelace-style/shoelace/dist/assets/icons public/assets",
        "update-api-client": "npx swagger-typescript-api@13.0.3 --templates _swagger/api_templates -p http://0.0.0.0:8000/swagger-types/ -r -o ./src/features/api/models/__generated__ -n api.ts --module-name-first-tag --api-class-name ApiBase",
        "update-api-client:app": "npx swagger-typescript-api@13.0.3 --templates _swagger/app_templates -p http://127.0.0.1:8000/swagger-types/ -r -o ./src/__generated__ -n api.ts --clean-output --module-name-first-tag"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^2.2.3",
        "@shoelace-style/shoelace": "^2.16.0",
        "axios": "^1.7.2",
        "jwt-decode": "^4.0.0",
        "preact": "^10.19.6",
        "preact-router": "^4.1.2",
        "react-redux": "^9.1.0",
        "sanitize-html": "^2.13.0"
    },
    "devDependencies": {
        "@preact/preset-vite": "^2.8.2",
        "@types/react-redux": "^7.1.33",
        "@types/sanitize-html": "^2.13.0",
        "@typescript-eslint/eslint-plugin": "^6.2.0",
        "@typescript-eslint/parser": "^6.2.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-react": "^7.33.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "jsdom": "^24.1.0",
        "prettier": "^3.2.5",
        "typescript": "^5.2.2",
        "vite": "^5.2.0",
        "vitest": "^1.4.0"
    }
}
